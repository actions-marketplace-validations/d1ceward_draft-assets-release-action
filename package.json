{
  "name": "release-upload-action",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript template action",
  "engines": {
    "node": "18.6.0"
  },
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d1ceward/release-upload-action.git"
  },
  "keywords": [
    "release",
    "upload",
    "setup"
  ],
  "author": "d1ceward",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@actions/github": "~5.1.1"
  },
  "devDependencies": {
    "@types/node": "~18.11.0",
    "@typescript-eslint/parser": "~5.42.1",
    "@vercel/ncc": "~0.34.0",
    "eslint": "~8.27.0",
    "eslint-plugin-github": "~4.4.1",
    "eslint-plugin-jest": "~27.1.4",
    "jest": "~29.3.0",
    "js-yaml": "~4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "~29.0.3",
    "typescript": "~4.8.4"
  }
}
